! To reread your .Xresources file, and keep your old resources: 
! xrdb ~/.Xresources

! Cheat sheet https://wiki.archlinux.org/index.php/X_resources#Basic_syntax
! Xcursor --------------------------------------------------------------------
Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size:  22

! xpdf -----------------------------------------------------------------------
xpdf*enableFreetype:    yes
xpdf*antialias:         yes
